<?xml version="1.0"?>
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Store:etc/config.xsd">
    <default>
        <customer>
            <captcha>
                <shown_to_logged_in_user>
                    <custom_form>1</custom_form>
                </shown_to_logged_in_user>
                <always_for>
                    <request_quote_form>1</request_quote_form>
                </always_for>
            </captcha>
        </customer>
        <msp_securitysuite_recaptcha translate="label">
            <frontend>
                <areas>
                    <request_quote_form>
                        <label>Request a Quote Form</label>
                    </request_quote_form>
                </areas>
            </frontend>
        </msp_securitysuite_recaptcha>
    </default>
</config>